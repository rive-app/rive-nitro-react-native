{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": ["rive"],
  "ios": {
    "iosModuleName": "RNRive"
  },
  "android": {
    "androidNamespace": ["rive"],
    "androidCxxLibName": "rive"
  },
  "autolinking": {
    "Rive": {
      "swift": "Rive",
      "kotlin": "Rive"
    },
    "RiveFileFactory": {
      "swift": "HybridRiveFileFactory",
      "kotlin": "HybridRiveFileFactory"
    },
    "RiveFile": {
      "swift": "HybridRiveFile",
      "kotlin": "HybridRiveFile"
    },
    "RiveView": {
      "swift": "HybridRiveView",
      "kotlin": "HybridRiveView"
    },
    "RiveImageFactory": {
      "swift": "HybridRiveImageFactory",
      "kotlin": "HybridRiveImageFactory"
    }
  },
  "ignorePaths": ["node_modules"]
}
